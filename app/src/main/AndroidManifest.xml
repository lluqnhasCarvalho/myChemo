<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lluqn.mychemo">

   <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_chemo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".registro.ActivityLogin" />
        <activity android:name=".registro.ActivityReset" />
        <activity android:name=".registro.ActivityNovo" />
        <activity android:name=".registro.ActivityCadastro" />
        <activity android:name=".conta.ActivityPerfil" />
        <activity android:name=".conta.ActivityGrafico" />
        <activity android:name=".conta.ActivityStatus" />
        <activity android:name=".conta.ActivityChat"/>

    </application>

</manifest>